{#
/**
 * @file
 * Default theme implementation for page titles.
 *
 * Available variables:
 * - title_attributes: HTML attributes for the page title element.
 * - title_prefix: Additional output populated by modules, intended to be
 *   displayed in front of the main title tag that appears in the template.
 * - title: The page title, for use in the actual content.
 * - title_suffix: Additional output populated by modules, intended to be
 *   displayed after the main title tag that appears in the template.
 *
 * @ingroup templates
 *
 * @see template_preprocess_page_title()
 */
#} 
{% if is_front %}
    <div class="page-header-wrapper">
        <h1{{ title_attributes.addClass('page-header') }}>{{ title }}</h1>
    </div>
{% endif %}
{% if not is_front %}
    {% if node.field_banner_image.entity.field_media_image.entity.uri.value %}
    <div class="banner-wrapper 1" style="background-image: url('{{ file_url(content.field_banner_image[0]['#media'].field_media_image.entity.uri.value) }}');"> 
    {% elseif field_banner_image %}
    <div class="banner-wrapper 2" style="background-image: url('{{ field_banner_image }}');"> 
    {% else %}
    <div class="banner-wrapper 3" style="background-image: url('/themes/custom/vermontgold/images/Banner1.jpg');">
    {% endif %}
    {% if title %}
    <div class="page-header-wrapper">
        <h1{{ title_attributes.addClass('page-header') }}>{{ title }}</h1>
        {% if node.getCreatedTime %}
            <div class="info">
                <span class="date">{{ node.getCreatedTime|format_date('custom','F j, Y') }} {{ date }}</span>
                {# {% if field_categories %} #}
                    <span> | {{ node.field_categories[0]['#title']}} || {{ node.field_categories.entityfield_categories.entity[0]['#title']}} ||| {% if node.field_tags[0] %} 
    <ul>
        {% for key, item in node.field_tags if key|first != '#' %}
        <li>{{ item['#title'] }}</li>
        {% endfor %}
    </ul>
    {% endif %} |||| {{ node.field_tags.0.target_id }}</span>
                {# {% endif %} #}
            </div>
        {% endif %}
    </div>
    {% endif %}
    </div>
   {% endif %}