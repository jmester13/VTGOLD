{#
/**
 * @file
 * Default theme implementation for page titles.
 *
 * Available variables:
 * - title_attributes: HTML attributes for the page title element.
 * - title_prefix: Additional output populated by modules, intended to be
 *   displayed in front of the main title tag that appears in the template.
 * - title: The page title, for use in the actual content.
 * - title_suffix: Additional output populated by modules, intended to be
 *   displayed after the main title tag that appears in the template.
 *
 * @ingroup templates
 *
 * @see template_preprocess_page_title()
 */
#} 
    {% if node.field_media_image.entity.field_media_image.entity.uri.value %}
    <div class="banner-wrapper" style="background-image: url('{{ file_url(node.field_media_image.entity.field_media_image.entity.uri.value) }}');"> 
    {% elseif banner_image %}
    <div class="banner-wrapper" style="background-image: url('{{ banner_image }}');"> 
    {% else %}
    <div class="banner-wrapper" style="background-image: url('/themes/custom/vermontgold/images/Banner2.jpg');">
    {% endif %}
    {% if title %}
    <div class="page-header-wrapper">
        <h1{{ title_attributes.addClass('page-header') }}>{{ title }}</h1>
        {% if node.getType == 'article' %}
            {% if node.getCreatedTime %}
                <div class="info">
                    <span class="date">{{ node.getCreatedTime|format_date('custom','F j, Y') }} {{ date }}</span>
                    {# {% if field_categories %} #}
                        {# <span> {{node.field_categories}} </span> #}
                    {# {% endif %} #}
                </div>
            {% endif %}
        {% endif %}
    </div>
    {% endif %}
    </div>